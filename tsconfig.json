{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./dist",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/dashboard/layout.jsx",
    "src/app/dashboard/dashboard/layout.tsx",
    "src/app/ui/dashboard/sidebar/menuLink/menuLink.tsx",
    "src/app/pages/api/submit-data.ts",
    "src/app/lib/mongodb.js",
    "src/app/lib/mongodb.js"
, "src/app/lib/db.js"  ],
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node",
    "exclude": [
      "node_modules"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
